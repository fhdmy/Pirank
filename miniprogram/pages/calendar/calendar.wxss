.box1 .dateBox{
  width: 100%;
  height: 80rpx;
  line-height: 50rpx;
  text-align: center;
  margin-top: 50rpx;
  font-size: 40rpx;
}

.box1{
  display: flex;
  flex-wrap: wrap;
  margin: 0 auto;
  width:100%;
}

.box1>view{
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  font-size: 34rpx;
}

.dateOn{
  border-radius: 50%;
  border: 1px solid #24b9ff;
  /* background-color: hotpink; */
  /* color: #fff; */
}
.datefinish{
  border-radius: 50%;
  background-color: hotpink;
  color: #fff;
}

.left-btn{
  display: inline-block;
  height: 60rpx;
  width:80rpx;
  float: left;
  font-size: 30rpx;
  padding:0;
}

.right-btn{
  display: inline-block;
  height: 60rpx;
  width:80rpx;
  float: right;
  font-size: 30rpx;
  padding:0;
}
